{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules|test|dist)"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "site",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/minami"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": false
    }
  }
}
